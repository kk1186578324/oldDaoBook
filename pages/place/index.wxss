
.container{
  width: 100%;
  background: #eee;
}
.msg-title{
  font-weight: 600;
}
.contanier{
  width:100%;
}
.footer{
  position: fixed;
  bottom:0;
  width: 100%;
  margin: 0 auto;
  background: white;
  display: flex;
  flex-direction: row;
  align-items: center;
  box-shadow: 1px 1px 1px 1px;
  justify-content: space-between;
}
.footer .all{
  margin-left:50rpx;
}
.footer .pay{
  background: red;
  color: white;
  padding: 30rpx 100rpx;
}
.check-all{
  margin-left:10rpx;
}
 
.touch-move-active .content, .touch-move-active .del {
  -webkit-transform: translateX(0);
  transform: translateX(0);
}
.address-head{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}
.address-head text{
  font-weight: 700;
}
.address-contarier{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between
}
.addres-nav{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between
}
.weui-select{
  border-right:none;
}
.fear{
  color: red;
}

 